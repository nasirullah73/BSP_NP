PL/SQL Developer Report

[SQL]
SELECT-- Mon AS "Invoice Date",TRANSNO as "Invoice #",
CUSTOMERID,ORGNAME as "Customer Name",REGIONNAME,ADDRESS
,CNIC,NTN
--,REGIONNAME as "Customer Town"
--,PRODNAME as "Iten Name",UNIT as "Item Size",UNIT_PRICE as "Trade Price"
--,SUM(SALE_QTY) as "Sales Qty"
--,SUM(BONUS_QTY) as "Sales Bon",SUM(SALE_RETURN) as "Ret Qty"
--,SUM(BONUS_RETURN) AS "Ret Bon",SUM(DIS) as "Disc"
,SUM((NVL(SALE_QTY,0)-NVL(SALE_RETURN,0))*UNIT_PRICE) AS Net_Amt
--,SUM(ST) as "S TAX"
,TAX_P
,--SUM((NVL(SALE_QTY,0)-NVL(SALE_RETURN,0))*UNIT_PRICE)+
((SUM((NVL(SALE_QTY,0)-NVL(SALE_RETURN,0))*UNIT_PRICE))*NVL(TAX_P,0)/100) TAX_AMT FROM
(
SELECT TO_CHAR(M.TRANSDATE,'DD/MM/YY') as Mon,M.TRANSNO,C.Customerid,C.ORGNAME,C.CNIC,C.NTN,C.ADDRESS,R.REGIONNAME,P.PRODNAME,P.UNIT,D.UNIT_PRICE
,SUM(D.SALE_QTY) SALE_QTY,SUM(D.BONUS_QTY) BONUS_QTY,0 SALE_RETURN,0 BONUS_RETURN,SUM(D.DISCOUNT_AMT) DIS
,SUM(D.SALETAX_AMT) ST,NVL(M.TAX_P,0) TAX_P
FROM SALE_MASTER M,SALE_DETAIL D,CUSTOMERS C,PRODUCTS P,COMPGROUPS CG,REGION R
  WHERE M.TRANSNO=D.TRANSNO
    AND M.CUSTOMERID=C.CUSTOMERID
    AND M.REGIONID=R.REGIONID
    AND D.COMPANY_ID=P.COMPANY_ID
    AND D.PRODUCTID=P.PRODUCTID
    AND P.COMPANY_ID=CG.COMPNAY_ID
    AND P.GROUPID=CG.GROUPID
    --AND D.COMPANY_ID=&comp
    AND TRUNC(M.TRANSDATE) BETWEEN &F_DATE AND &T_DATE
    --AND D.PRODUCTID='001'
    GROUP BY TO_CHAR(M.TRANSDATE,'DD/MM/YY'),M.TRANSNO,C.Customerid,C.ORGNAME,C.CNIC,C.NTN,C.ADDRESS,R.REGIONNAME,P.PRODNAME,P.UNIT,D.UNIT_PRICE,M.TAX_P
    UNION ALL
    SELECT TO_CHAR(M.TRANSDATE,'DD/MM/YY') as Mon,M.TRANSNO,C.Customerid,C.ORGNAME,C.CNIC,C.NTN,C.ADDRESS,R.REGIONNAME,P.PRODNAME,P.UNIT,P.UNITPRICE
,0 SALE_QTY,0 BONUS_QTY,SUM(D.SALE_QTY) SALE_RETURN,SUM(D.BONUS_QTY) BONUS_RETURN,SUM(D.DISCOUNT_AMT) DIS
,SUM(D.SALETAX_AMT) ST,NVL(M.TAX_P,0) TAX_P
FROM RETURN_MASTER M,RETURN_DETAIL D,CUSTOMERS C,PRODUCTS P,COMPGROUPS CG,REGION R
  WHERE M.TRANSNO=D.TRANSNO
    AND M.CUSTOMERID=C.CUSTOMERID
    AND M.REGIONID=R.REGIONID
    AND D.COMPANY_ID=P.COMPANY_ID
    AND D.PRODUCTID=P.PRODUCTID
    AND P.COMPANY_ID=CG.COMPNAY_ID
    AND P.GROUPID=CG.GROUPID
   -- AND D.COMPANY_ID=&comp
    AND TRUNC(M.TRANSDATE) BETWEEN &F_DATE AND &T_DATE
    --AND D.PRODUCTID='001'
    GROUP BY TO_CHAR(M.TRANSDATE,'DD/MM/YY'),M.TRANSNO,C.Customerid,C.ORGNAME,C.CNIC,C.ADDRESS,C.NTN,R.REGIONNAME,P.PRODNAME,P.UNIT,P.UNITPRICE,M.TAX_P
   
    )
    GROUP BY --MON,TRANSNO,
    CUSTOMERID,ORGNAME,CNIC,NTN,REGIONNAME,ADDRESS--,PRODNAME,UNIT,UNIT_PRICE
    ,TAX_P
    ORDER BY 1,2,3,4,5;

